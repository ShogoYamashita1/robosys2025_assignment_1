#!/usr/bin/python3
# SPDX-FileCopyrightText: 2025 Shogo Yamashita
import sys
import random


def shuffle(rn):
    return random.randint(0, rn)

count = 0

# 改行文字を除いた文字数をカウント
for line in sys.stdin:
    count+=1

rand_num = [0] * (count * 2)

for i in range(count * 2):
    rand_num[i] = shuffle(count-1)
    print(f"rand_num%d: %d" % (i, rand_num[i]))

print("count: ",count)
